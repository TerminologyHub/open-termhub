  "description": "A concept map to translate marital status codes from a custom code system to HL7 FHIR v3-MaritalStatus codes.",
  "sourceUri": "http://example.org/fhir/CodeSystem/custom-marital-status",
  "targetUri": "http://terminology.hl7.org/CodeSystem/v3-MaritalStatus",
  "group": [
    {
      "source": "http://example.org/fhir/CodeSystem/custom-marital-status",
      "target": "http://terminology.hl7.org/CodeSystem/v3-MaritalStatus",
      "element": [
        {
          "code": "S",
          "display": "Single",
          "target": [
            {
              "code": "S",
              "display": "Never Married",
              "equivalence": "equivalent"
            }
          ]
        },
        {
          "code": "M",
          "display": "Married",
          "target": [
            {
              "code": "M",
              "display": "Married",
              "equivalence": "equivalent"
            }
          ]
        },
        {
          "code": "D",
          "display": "Divorced",
          "target": [
            {
              "code": "D",
              "display": "Divorced",
              "equivalence": "equivalent"
            }
          ]
        },
        {
          "code": "W",
          "display": "Widowed",
          "target": [
            {
              "code": "W",
              "display": "Widowed",
              "equivalence": "equivalent"
            }
          ]
        },
        {
          "code": "U",
          "display": "Unknown",
          "target": [
            {
              "code": "UNK",
              "display": "Unknown",
              "equivalence": "equivalent"
            }
          ]
        }
      ]
    }
  ]
